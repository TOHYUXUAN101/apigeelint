<APIProxy name="SampleProxy">
  <Description>Sample Apigee proxy for lint and security testing</Description>

  <Policies>
    <Policy>OAuthV2-OAuth</Policy>
    <Policy>OAuthV2-GenerateAccessToken</Policy>
    <Policy>Quota</Policy>
    <Policy>SpikeArrest</Policy>
    <Policy>JSONThreat-Protection</Policy>
    <Policy>xmltp-XMLThreat-Protection</Policy>
    <Policy>CORS</Policy>
    <Policy>AssignMessage-ContentType</Policy>
    <Policy>AssignMessage-SecurityHeaders</Policy>
    <Policy>MessageLogging-Hygiene</Policy>
    <Policy>RaiseFault-AuthError.</Policy>
    <Policy>RaiseFault-Generic.</Policy>
    <Policy>RaiseFault-ThreatError.</Policy>
  </Policies>

  <ProxyEndpoints>
    <ProxyEndpoint>default</ProxyEndpoint>
  </ProxyEndpoints>

  <TargetEndpoints>
    <TargetEndpoint>default</TargetEndpoint>
  </TargetEndpoints>
</APIProxy>
